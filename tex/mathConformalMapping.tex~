\draw(0,0)node[ocirc]{};
%
\foreach \x in {-0.5,0.5,1}{\draw(\x,-1.25)--(\x,1.25);}
\foreach \y in {-1,-0.5,0.5,1}{\draw(-2.25,\y)--(1.5,\y);}
