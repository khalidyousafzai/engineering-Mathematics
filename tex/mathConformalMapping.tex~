\draw[stealth-](-1,0)++(115:1) to [out=130,in=-70]++(-0.3,0.3)node[above]{$x=-\tfrac{1}{2}$};
\draw[stealth-](0,1)++(45:1) to [out=60,in=180]++(0.3,0.3)node[right]{$y=-\tfrac{1}{2}$};
