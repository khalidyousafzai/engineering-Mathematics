\draw(0,-0.75)node[below left]{$-1$};
\draw(0,0.75)node[above left]{$1$};
\draw(-1,0)node[above left]{$D$}node[below left]{$-\tfrac{\pi}{2}$};
\draw(1,0)node[above right]{$A$}node[below right]{$\tfrac{\pi}{2}$};
\end{tikzpicture}
\end{subfigure}\hfill
