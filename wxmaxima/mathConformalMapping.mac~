print("ex 15.9 fig w=1/z");

z:x+%i*y;
w:1/z;
uu:realpart(w);
vv:imagpart(w);

eqa:u=ev(uu,y=-1/2);
eqb:v=ev(vv,y=-1/2);
solve([eqa,eqb],[x]);

eqa:u=ev(uu,y=-1);
eqb:v=ev(vv,y=-1);
solve([eqa,eqb],[x]);

eqa:u=ev(uu,y=-2);
eqb:v=ev(vv,y=-2);
solve([eqa,eqb],[x]);

print("=======================");
kill(all);
print( " ex 15.12 fig w=(z+i)/(-iz+1)");

z:x+%i*y;
w:(z-%i)/(-%i*z+1);
expand(%);
uu:realpart(w);
vv:imagpart(w);

eqa:u=ev(uu,y=0);
eqb:v=ev(vv,y=0);
solve([eqa,eqb],[x,u]);

eqa:u=ev(uu,y=1/2);
eqb:v=ev(vv,y=1/2);
solve([eqa,eqb],[x,u]);

eqa:u=ev(uu,x=1/2);
eqb:v=ev(vv,x=1/2);
solve([eqa,eqb],[y,u]);

eqa:u=ev(uu,x=1);
eqb:v=ev(vv,x=1);
solve([eqa,eqb],[y,u]);

eqa:u=ev(uu,x=2);
eqb:v=ev(vv,x=2);
solve([eqa,eqb],[y,u]);


print("=======================");
kill(all);
print( " ex 15.13 fig w=(2z-1)/(z-2)");

z:x+%i*y;
w:(2*z-1)/(z-2);
expand(%);
uu:realpart(w);
vv:imagpart(w);

eqa:u=ev(uu,x=1/2);
eqb:v=ev(vv,x=1/2);
solve([eqa,eqb],[y,u]);

eqa:u=ev(uu,x=0);
eqb:v=ev(vv,x=0);
solve([eqa,eqb],[y,u]);

eqa:u=ev(uu,x=-1/2);
eqb:v=ev(vv,x=-1/2);
solve([eqa,eqb],[y,u]);
