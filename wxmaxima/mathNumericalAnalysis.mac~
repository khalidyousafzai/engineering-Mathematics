print("Q 21.87");
load("mnewton");

f(x,y):=y;
x0:0;
y0:1;
h:0.01;
nsteps:10;

xeuler:makelist(0,i,1,nsteps+1,1);
yeuler:makelist(0,i,1,nsteps+1,1);

xeuler[1]:x0;
yeuler[1]:y0;

for i:1 thru nsteps do (
xeuler[i+1]:xeuler[i]+h,
yeuler[i+1]:yeuler[i]+h*f(xeuler[i],yeuler[i])
);
yeuler;

